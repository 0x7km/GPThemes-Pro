		"style chatgpt",
		"modern themes",
		"minimalistic"
	],
	"license": "GPL-3.0",
	"devDependencies": {
		"@parcel/transformer-sass": "^2.10.3",
		"parcel": "^2.10.3"
	},
	"browserslist": "> 0.5%, last 2 versions, not dead"
}
