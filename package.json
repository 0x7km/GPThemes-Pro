{
	"name": "gpthemes",
	"version": "1.2.1",
	"description": "GPThemes is a Chrome extension designed to improve the visual experience of ChatGPT by injecting custom CSS to enhance its theme.",
	"repository": {
		"type": "git",
		"url": "https://github.com/itsmartashub/GPThemes.git"
	},
	"bugs": {
		"url": "https://github.com/itsmartashub/GPThemes/issues"
	},
	"homepage": "https://github.com/itsmartashub/GPThemes/",
	"scripts": {
		"test": "parcel src/sass/index.scss --dist-dir extension-test/css --no-source-maps",
		"dev": "parcel src/sass/index.scss --dist-dir extension-dev/css --no-source-maps",
		"build": "parcel build src/sass/index.scss --dist-dir extension-prod/css --no-source-maps",
		"hybrid": "parcel src/popup.html --dist-dir extension-hybrid/ --no-source-maps"
	},
	"author": "Marta Bires",
	"keywords": [
		"chatgpt",
		"chat",
		"openai",
		"gpt",
		"ai",
		"extension",
		"themes",
		"chrome",
		"style",
		"chatgpt themes",
		"style chatgpt",
		"modern themes",
		"minimalistic",
		"chatgpt",
		"gpthemes",
		"chrome extension",
		"ui customization",
		"theme enhancement",
		"custom css",
		"modern ui",
		"visual experience",
		"chatgpt themes",
		"browser extension",
		"user interface makeover",
		"personalization",
		"stylish design",
		"refreshed appearance",
		"visual upgrade",
		"user-friendly",
		"chatgpt enhancement",
		"css injection",
		"chrome web store",
		"web design makeover"
	],
	"license": "GPL-3.0",
	"devDependencies": {
		"@parcel/config-webextension": "^2.11.0",
		"@parcel/transformer-sass": "^2.10.3",
		"parcel": "^2.10.3",
		"parcel-reporter-static-files-copy": "^1.5.3",
		"postcss": "^8.4.32",
		"postcss-preset-env": "^9.3.0",
		"postcss-transform": "^0.0.2",
		"webextension-polyfill": "^0.10.0"
	},
	"browserslist": "> 0.5%, last 2 versions, not dead",
	"staticFiles": [
		{
			"staticPath": "./src/static/",
			"distDir": "extension-dev",
			"env": {
				"NODE_ENV": "development"
			}
		},
		{
			"staticPath": "./src/static/",
			"distDir": "extension-prod",
			"env": {
				"NODE_ENV": "production"
			}
		}
	]
}
