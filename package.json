{
	"name": "gpthemes",
	"displayName": "GPThemes - ChatGPT Restyled",
	"version": "2.0.2",
	"description": "GPThemes is a Chrome extension designed to improve the visual experience of ChatGPT by injecting custom CSS to enhance its theme.",
	"repository": {
		"type": "git",
		"url": "https://github.com/itsmartashub/GPThemes.git"
	},
	"bugs": {
		"url": "https://github.com/itsmartashub/GPThemes/issues"
	},
	"homepage": "https://github.com/itsmartashub/GPThemes/",
	"scripts": {
		"clean:dev": "rimraf .parcel-cache extension-dev",
		"clean:prod": "rimraf .parcel-cache extension-prod",
		"clean:zips": "rimraf extension-zips",
		"dev": "parcel watch src/manifests/chrome/manifest.json --host localhost --dist-dir extension-dev/chromium",
		"dev:ff": "parcel watch src/manifests/firefox-mv2/manifest.json --host localhost --dist-dir extension-dev/firefox-mv2",
		"build": "parcel build src/manifests/chrome/manifest.json --dist-dir extension-prod/gpthemes-chromium --no-source-maps",
		"build:ff": "parcel build src/manifests/firefox-mv2/manifest.json --dist-dir extension-prod/gpthemes-firefox-mv2 --no-source-maps",
		"zip": "node ./scripts/zip.js",
		"build:all": "npm run clean-prod && npm run clean-zips && npm run build && npm run build-ff && npm run zip"
	},
	"author": "itsmarta",
	"keywords": [
		"chatgpt",
		"chat",
		"openai",
		"gpt",
		"ai",
		"extension",
		"themes",
		"chrome",
		"style",
		"chatgpt themes",
		"style chatgpt",
		"modern themes",
		"minimalistic",
		"gpthemes",
		"chrome extension",
		"ui customization",
		"theme enhancement",
		"custom css",
		"modern ui",
		"visual experience",
		"chatgpt themes",
		"browser extension",
		"user interface makeover",
		"personalization",
		"stylish design",
		"refreshed appearance",
		"visual upgrade",
		"user-friendly",
		"chatgpt enhancement",
		"css injection",
		"chrome web store",
		"web design makeover",
		"firefox",
		"firefox addons"
	],
	"license": "GPL-3.0",
	"devDependencies": {
		"@parcel/config-webextension": "^2.12.0",
		"@parcel/transformer-sass": "^2.12.0",
		"adm-zip": "^0.5.10",
		"parcel": "^2.12.0",
		"postcss": "^8.4.35",
		"postcss-discard-unused": "^6.0.3",
		"postcss-preset-env": "^9.4.0",
		"postcss-transform": "^1.0.0",
		"rimraf": "^5.0.5",
		"webextension-polyfill": "^0.10.0"
	},
	"browserslist": "> 0.5%, last 2 versions, not dead"
}
