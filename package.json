{
	"name": "gpthemes",
	"version": "1.3.0",
	"description": "GPThemes is a Chrome extension designed to improve the visual experience of ChatGPT by injecting custom CSS to enhance its theme.",
	"repository": {
		"type": "git",
		"url": "https://github.com/itsmartashub/GPThemes.git"
	},
	"bugs": {
		"url": "https://github.com/itsmartashub/GPThemes/issues"
	},
	"homepage": "https://github.com/itsmartashub/GPThemes/",
	"scripts": {
		"dev": "parcel watch src/manifests/chrome/manifest.json --host localhost --dist-dir extension-dev/chromium",
		"dev_ff": "parcel watch src/manifests/firefox-mv2/manifest.json --host localhost --dist-dir extension-dev/firefox-mv2",
		"build": "parcel build src/manifests/chrome/manifest.json --dist-dir extension-prod/chromium --no-source-maps",
		"build_ff": "parcel build src/manifests/firefox-mv2/manifest.json --dist-dir extension-prod/firefox-mv2 --no-source-maps"
	},
	"author": "itsmarta",
	"keywords": [
